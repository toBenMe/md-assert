      if (this.ifThRepeatLabel(v.target.value)) {\n            thisCell.innerText = '数据重复';\n            _util_js__WEBPACK_IMPORTED_MODULE_2__.tryAddClassName(thisCell, 'error');\n          } else {\n            thisCell.innerText = v.target.value;\n            _util_js__WEBPACK_IMPORTED_MODULE_2__.removeClassName(thisCell, 'error');\n          }\n        }\n      } else if (colIndex == 0) {\n        if (v.target.value == '') {\n          this.clearTrNames(thisCell);\n        } else {\n          if (this.ifTrRepeatLabel(v.target.value)) {\n            thisCell.innerText = '数据重复';\n            _util_js__WEBPACK_IMPORTED_MODULE_2__.tryAddClassName(thisCell, 'error');\n          } else {\n            thisCell.innerText = v.target.value;\n            _util_js__WEBPACK_IMPORTED_MODULE_2__.removeClassName(thisCell, 'error');\n          }\n        }\n      } else {\n        if (!this.isScatterType()) {\n          if (colIndex !== 0 && rowIndex !== 0) {\n            if (isNaN(Number(v.target.value))) {\n              _util_js__WEBPACK_IMPORTED_MODULE_2__.tryAddClassName(thisCell, 'error');\n            } else _util_js__WEBPACK_IMPORTED_MODULE_2__.removeClassName(thisCell, 'error');\n          }\n        }\n        thisCell.innerText = v.target.value;\n      }\n      if (_util_js__WEBPACK_IMPORTED_MODULE_2__.ifEllipsis(thisCell)) thisCell.setAttribute('original-title', _util_js__WEBPACK_IMPORTED_MODULE_2__.filterXss(thisCell.innerText));else thisCell.removeAttribute('original-title');\n      this.changeChartData({\n        type: 'cell',\n        row: rowIndex,\n        col: colIndex,\n        value: v.target.value\n      }, callback);\n    });\n  }\n  changeLegend(e, callback = null) {\n    e.stopPropagation();\n    this.Options.legend = e.target.checked;\n    if (e.target.checked) {\n      this.Options.legend = {\n        layout: 'horizontal',\n        position: 'bottom',\n        flipPage: false\n      };\n    } else this.Options.legend = false;\n    this.draw();\n    if (callback) callback({\n      legend: this.Options.legend\n    });\n  }\n  changeTableLayout(params) {\n    const {\n      rowIndex,\n      colIndex,\n      thisRow\n    } = params;\n    const table = document.querySelector('#po-chartdata-container>table');\n    const [rowLen, colLen, newRow] = [table.rows.length, table.rows[0].cells.length, thisRow.cloneNode(true)];\n    newRow.querySelectorAll('td').forEach(item => _util_js__WEBPACK_IMPORTED_MODULE_2__.removeClassName(item, 'current'));\n    if (rowIndex === rowLen - 1) {\n      if ((colIndex !== colLen - 1 || this.ifFixedTable()) && colIndex !== 0) {\n        if (thisRow.cells[0].innerText === '') this.clearTrNames(thisRow.cells[0]);\n      }\n      table.querySelector('tbody').appendChild(newRow);\n      document.querySelector('#po-chartdata-container table').scrollIntoView({\n        block: 'end',\n        inline: 'nearest',\n        behavior: 'smooth'\n      });\n      this.delIconAllowed();\n    } else if (colIndex === colLen - 1 && !this.ifFixedTable()) {\n      if (rowIndex !== rowLen - 1 && rowIndex !== 0) {\n        const fRowlColCell = table.querySelector('tr:first-child>td:last-child');\n        if (fRowlColCell.innerText === '') this.clearThNames(fRowlColCell);else fRowlColCell.innerText = '';\n      }\n      const col = document.createElement('col');\n      const tableWidth = this.TableContainer.offsetWidth;\n      let colWidth = (tableWidth - TABLE_PADDING * 2) / (colLen + 1);\n      if (colWidth < 54) colWidth = 54;\n      table.querySelector('colgroup').appendChild(col);\n      table.querySelectorAll('colgroup>col').forEach(item => {\n        item.setAttribute('width', colWidth + 'px');\n      });\n      table.querySelectorAll('tbody>tr').forEach(item => {\n        const newCell = document.createElement('td');\n        item.appendChild(newCell);\n      });\n      document.querySelector('#po-chartdata-container table').scrollIntoView({\n        block: 'nearest',\n        inline: 'end',\n        behavior: 'smooth'\n      });\n      this.delIconAllowed();\n    }\n    table.querySelectorAll('td').forEach(item => {\n      if (_util_js__WEBPACK_IMPORTED_MODULE_2__.isMobi()) {\n        item.removeEventListener('touchstart', this.chartDataDblClickEvent);\n        item.addEventListener('touchstart', this.chartDataDblClickEvent);\n      } else {\n        item.removeEventListener('click', this.chartDataDblClickEvent);\n        item.addEventListener('click', this.chartDataDblClickEvent);\n        item.removeEventListener('dblclick', this.chartDataDblClickEvent);\n        item.addEventListener('dblclick', this.chartDataDblClickEvent);\n      }\n    });\n  }\n  dataFormat(options = this.Options) {\n    //grid: 'null' => grid: null\n    for (let n in options) {\n      if (Object.prototype.toString.call(options[n]) === '[object Object]') {\n        this.dataFormat(options[n]);\n      } else {\n        if (options[n] === 'null') options[n] = null;\n      }\n    }\n  }\n  bindDelEvent(type, callback) {\n    const table = document.querySelector('#po-chartdata-container>table');\n    const cell = table.querySelector('td.current');\n    if (cell) {\n      const rowIndex = cell.parentElement.rowIndex;\n      const colIndex = cell.cellIndex;\n      if (type === 'clear') {\n        if (rowIndex === 0) {\n          this.clearThNames(cell);\n        } else if (colIndex === 0) {\n          this.clearTrNames(cell);\n        } else cell.innerText = '';\n      } else if (type === 'row') {\n        const thisRow = table.querySelector('tbody>tr:nth-child(' + (rowIndex + 1) + ')');\n        thisRow.parentNode.removeChild(thisRow);\n        this.delIconNotAllowed();\n      } else if (type === 'col') {\n        const tr = table.querySelectorAll('tbody>tr');\n        Array.from(tr).forEach(item => {\n          const thisColTd = item.querySelector('td:nth-child(' + (colIndex + 1) + ')');\n          thisColTd.parentNode.removeChild(thisColTd);\n        });\n        const colgroup = table.querySelector('colgroup');\n        const thiscolgroup = colgroup.querySelector('col:nth-child(' + (colIndex + 1) + ')');\n        thiscolgroup.parentNode.removeChild(thiscolgroup);\n        this.delIconNotAllowed();\n      }\n      this.changeChartData({\n        type: 'all'\n      }, callback);\n    }\n  }\n  draw() {\n    //渲染图形\n    if (this.ChartObj) this.ChartObj.destroy();\n    Array.from(this.ChartDom.children).forEach(item => {\n      if (item.tagName == 'CANVAS') item.remove();\n    });\n    this.refreshAboutData();\n    this.initColors();\n    if (!this.isScatterType()) {\n      let data = JSON.parse(JSON.stringify(this.Options.data));\n      let _yField;\n      if (this.ChartType == 'bar') {\n        _yField = this.Options.xField;\n      } else if (this.ChartType == 'pie') {\n        _yField = this.Options.angleField;\n      } else {\n        _yField = this.Options.yField;\n      }\n      for (let i = 0; i < data.length; i++) {\n        let item = data[i][_yField];\n        data[i][_yField] = isNaN(Number(item)) ? 0 : item;\n      }\n      this.Options.data = data;\n    }\n    if (this.isLineType()) {\n      if (this.Options.label) {\n        this.Options.label.style.fill = null;\n        this.Options.label.formatter = (v, {\n          color\n        }) => v[this.Options.yField];\n      }\n    }\n    if (this.isBarType()) {\n      if (this.Options.label) {\n        if (this.Options.label.layout) {\n          this.Options.label.layout.push({\n            type: 'limit-in-canvas'\n          });\n        } else {\n          this.Options.label.layout = [{\n            type: 'limit-in-canvas'\n          }];\n        }\n      }\n    }\n    this.ChartObj = this.bindNewEvent(this.ChartType, this.ChartDom, this.Options);\n    this.ChartObj.render();\n\n    // tooltip的显示定位需要外层relative容器\n\n    let canvas = this.ChartObj.chart.getCanvas().get('el');\n    canvas.setAttribute('class', 'charts shape_canvas');\n    if (this.Options.propsx) canvas.setAttribute('propsx', this.Options.propsx);\n    if (this.Options.propsy) canvas.setAttribute('propsy', this.Options.propsy);\n    if (this.Options.propspartx) canvas.setAttribute('propspartx', this.Options.propspartx);\n    if (this.Options.propsparty) canvas.setAttribute('propsparty', this.Options.propsparty);\n    if (this.Options?.removeBox) {\n      //去掉canvas外层容器\n      canvas.parentNode.className = 'delChartsContainer';\n      this.ChartDom.appendChild(canvas);\n      if (this.ChartDom.querySelector('.delChartsContainer')) this.ChartDom.querySelector('.delChartsContainer').remove();\n    }\n    //给图形绑定点击事件，项目上大部分不用\n    this.changeChartConfig();\n  }\n  initColors() {\n    let seriesLen;\n    let series = [];\n    const seriesField = this.Options?.seriesField || this.Options?.colorField;\n    if (!seriesField) seriesLen = 1;else {\n      this.Options.data.map(item => {\n        if (!series.includes(item[seriesField])) series.push(item[seriesField]);\n      });\n      seriesLen = series.length;\n    }\n    if (Array.isArray(this.Options?.color) && this.Options.color[0].length > 23) {\n      //Array.isArray(this.Options?.color) && this.Options.color[0].includes(':')\n      //3d颜色判断\n      const color = 'r(0.4, 0.3, 0.7) 0:rgba(255,255,255,0.5) 1:';\n      if (seriesLen == 1) {\n        this.Options.color = color + _data_js__WEBPACK_IMPORTED_MODULE_1__.defaultColors[0];\n      } else {\n        let i = 0;\n        this.Options.color = [];\n        while (i < seriesLen) {\n          if (i > 19) this.Options.color.push(color + _data_js__WEBPACK_IMPORTED_MODULE_1__.defaultColors[i % 20]);else this.Options.color.push(color + _data_js__WEBPACK_IMPORTED_MODULE_1__.defaultColors[i]);\n          i++;\n        }\n      }\n    } else {\n      if (seriesLen == 1) this.Options.color = _data_js__WEBPACK_IMPORTED_MODULE_1__.defaultColors[0];else {\n        let i = 0;\n        this.Options.color = [];\n        while (i < seriesLen) {\n          if (i > 19) this.Options.color.push(_data_js__WEBPACK_IMPORTED_MODULE_1__.defaultColors[i % 20]);else this.Options.color.push(_data_js__WEBPACK_IMPORTED_MODULE_1__.defaultColors[i]);\n          i++;\n        }\n      }\n    }\n  }\n  bindNewEvent(type = 'line', dom, options) {\n    let obj = null;\n    switch (type) {\n      case 'column':\n        obj = new _antv_g2plot__WEBPACK_IMPORTED_MODULE_0__.Column(dom, options);\n        break;\n      case 'bar':\n        obj = new _antv_g2plot__WEBPACK_IMPORTED_MODULE_0__.Bar(dom, options);\n        break;\n      case 'scatter':\n        obj = new _antv_g2plot__WEBPACK_IMPORTED_MODULE_0__.Scatter(dom, options);\n        break;\n      case 'pie':\n        obj = new _antv_g2plot__WEBPACK_IMPORTED_MODULE_0__.Pie(dom, options);\n        break;\n      case 'radar':\n        obj = new _antv_g2plot__WEBPACK_IMPORTED_MODULE_0__.Radar(dom, options);\n        break;\n      default:\n        obj = new _antv_g2plot__WEBPACK_IMPORTED_MODULE_0__.Line(dom, options);\n        break;\n    }\n    // if(dom.children.length > 1){\n    //     if(dom.firstChild.nodeType != 3) dom.lastChild.remove();\n    // }\n    return obj;\n  }\n  changeChartConfig() {\n    if (this.ChartObj) {\n      const newDom = document.getElementById(\"configContainer\");\n      //绑定点击事件\n      this.ChartObj.on('plot:dblclick', () => this.createTableContainer());\n      // this.ChartObj.on('plot:touchstart', () => this.createTableContainer());\n      this.ChartObj.on('axis:click', args => this.createConfigModal(args, newDom));\n      this.ChartObj.on('plot:mouseenter', () => {\n        window.mouseInChart = true;\n      });\n      this.ChartObj.on('plot:mouseup', () => {\n        window.mouseInChart = true;\n      });\n      this.ChartObj.on('plot:touchstart', () => {\n        window.mouseInChart = true;\n      });\n      this.ChartObj.on('plot:touchend', () => {\n        window.mouseInChart = true;\n      });\n      this.ChartDom.oncontextmenu = function () {\n        return false;\n      };\n      document.addEventListener('click', () => {\n        newDom.style.display = 'none';\n        if (document.getElementById('del-icon-modal')) document.getElementById('del-icon-modal').style.display = 'none';\n        _util_js__WEBPACK_IMPORTED_MODULE_2__.removeClassName(document.querySelector('#po-chartdata-container table .current'), 'current');\n      });\n      [this.ChartDom, newDom].forEach(item => {\n        item.addEventListener('click', e => e.stopPropagation());\n      });\n    }\n  }\n  initDataConfig() {\n    this.TableData = this.TableData.length === 0 ? this.dataTransToTableData() : this.TableData;\n    if (!this.TableData || this.TableData === 0) return;\n    const data = this.TableData;\n    const table = document.querySelector('#po-chartdata-container>table');\n    const colgroup = table.querySelector('colgroup');\n    const tbody = table.querySelector('tbody');\n    let colLen;\n    const thData = data[0];\n    //初始化colgroup\n    if (this.ifFixedTable()) colLen = thData.length;else colLen = thData.length + 1;\n    const tableWidth = this.TableContainer.offsetWidth == 0 ? TABLE_WIDTH : this.TableContainer.offsetWidth;\n    table.style.width = tableWidth + 'px';\n    let colWidth = Math.floor((tableWidth - TABLE_PADDING * 2) / (colLen + 1));\n    if (colWidth < 54) colWidth = 54;\n    let colN = 0;\n    while (colN < colLen) {\n      const col = document.createElement('col');\n      col.setAttribute('width', colWidth + 'px');\n      colgroup.appendChild(col);\n      colN++;\n    }\n    //初始化数据列\n    for (let i = 0; i <= data.length; i++) {\n      const tr = document.createElement('tr');\n      for (let n = 0; n < colLen; n++) {\n        const td = document.createElement('td');\n        if (i === 0 && n === 0 || i === data.length) td.innerText = '';else td.innerText = data[i][n] ?? '';\n        if (isNaN(Number(td.innerText)) && i !== 0 && n !== 0 && !this.isScatterType()) {\n          _util_js__WEBPACK_IMPORTED_MODULE_2__.tryAddClassName(td, 'error');\n        }\n        tr.appendChild(td);\n      }\n      tbody.appendChild(tr);\n    }\n    tbody.querySelectorAll('td').forEach(item => {\n      if (_util_js__WEBPACK_IMPORTED_MODULE_2__.isMobi()) {\n        item.removeEventListener('touchstart', this.chartDataDblClickEvent);\n        item.addEventListener('touchstart', this.chartDataDblClickEvent);\n      } else {\n        item.removeEventListener('click', this.chartDataDblClickEvent);\n        item.addEventListener('click', this.chartDataDblClickEvent);\n        item.removeEventListener('dblclick', this.chartDataDblClickEvent);\n        item.addEventListener('dblclick', this.chartDataDblClickEvent);\n      }\n      if (_util_js__WEBPACK_IMPORTED_MODULE_2__.ifEllipsis(item)) item.setAttribute('title', _util_js__WEBPACK_IMPORTED_MODULE_2__.filterXss(item.innerText));\n    });\n  }\n  bindCellKeyUp(v) {\n    v.preventDefault();\n    v.stopPropagation();\n    const cell = document.querySelector('#po-chartdata-container td.current');\n    const inputDisplay = window.getComputedStyle(document.getElementById('po-chartdata-inputbox')).display;\n    if (cell && inputDisplay === 'none') {\n      let thisCell;\n      const colIndex = cell.cellIndex;\n      switch (v.key) {\n        case 'ArrowUp':\n          thisCell = cell.parentNode?.previousSibling?.childNodes[colIndex];\n          break;\n        case 'ArrowDown':\n          thisCell = cell.parentNode?.nextSibling?.childNodes[colIndex];\n          break;\n        case 'ArrowLeft':\n          thisCell = cell.previousSibling;\n          break;\n        case 'ArrowRight':\n          thisCell = cell.nextSibling;\n          break;\n        case 'Enter':\n          const clickEvent = document.createEvent('MouseEvents');\n          clickEvent.initEvent('dblclick', true, true);\n          cell.dispatchEvent(clickEvent);\n          break;\n        default:\n          break;\n      }\n      if (thisCell && v.key !== 'Enter') thisCell.click();\n    } else if (cell && inputDisplay === 'block') {\n      if (v.key === 'Enter') {\n        document.getElementById('po-chartdata-inputbox').style.display = 'none';\n      }\n    }\n  }\n  chartDataClickEvent(e) {\n    //单击表格单元格事件\n    const table = document.querySelector('#po-chartdata-container>table');\n    const cell = e.target;\n    const colIndex = cell.cellIndex;\n    const rowIndex = cell.parentElement.rowIndex;\n    const colLen = table.querySelectorAll('colgroup>col').length;\n    const rowLen = table.querySelectorAll('tbody>tr').length;\n    document.querySelectorAll('#po-chartdata-container>table td').forEach(item => {\n      _util_js__WEBPACK_IMPORTED_MODULE_2__.removeClassName(item, 'current');\n    });\n    _util_js__WEBPACK_IMPORTED_MODULE_2__.tryAddClassName(cell, 'current');\n    const delBtn = document.getElementById('del-icon');\n    delBtn.querySelectorAll('svg>path').forEach(item => {\n      if ((colIndex !== colLen - 1 || table.className === 'solid') && rowIndex !== rowLen - 1 && (rowIndex !== 0 || colIndex !== 0)) {\n        item.setAttribute('fill', '#212930');\n        _util_js__WEBPACK_IMPORTED_MODULE_2__.tryAddClassName(delBtn, 'active');\n      } else {\n        item.setAttribute('fill', '#C7C9CB');\n        _util_js__WEBPACK_IMPORTED_MODULE_2__.removeClassName(delBtn, 'active');\n      }\n    });\n  }\n  chartDataDblClickEvent(e) {\n    //双击表格单元格事件\n    e.stopPropagation();\n    const table = document.querySelector('#po-chartdata-container>table');\n    const cell = e.target;\n    const colLen = table.querySelectorAll('colgroup>col').length;\n    const rowLen = table.querySelectorAll('tbody>tr').length;\n    const inputBox = document.getElementById('po-chartdata-inputbox');\n    const input = inputBox.querySelector('input');\n    const targettd = e.target;\n    const rowIndex = targettd.parentElement.rowIndex;\n    const colIndex = targettd.cellIndex;\n    const oldCurrent = table.querySelector('td.current');\n    input.blur();\n    //合并单元格单击\n    document.querySelectorAll('#po-chartdata-container>table td').forEach(item => {\n      _util_js__WEBPACK_IMPORTED_MODULE_2__.removeClassName(item, 'current');\n    });\n    _util_js__WEBPACK_IMPORTED_MODULE_2__.tryAddClassName(cell, 'current');\n    const delBtn = document.getElementById('del-icon');\n    delBtn.querySelectorAll('svg>path').forEach(item => {\n      if ((colIndex !== colLen - 1 || table.className.includes('solid')) && rowIndex !== rowLen - 1 && (rowIndex !== 0 || colIndex !== 0) && !table.className.includes('scatter') && !table.className.includes('pie') || (table.className.includes('scatter') || table.className.includes('pie')) && rowIndex !== 0) {\n        item.setAttribute('fill', '#212930');\n        _util_js__WEBPACK_IMPORTED_MODULE_2__.tryAddClassName(delBtn, 'active');\n      } else {\n        item.setAttribute('fill', '#C7C9CB');\n        _util_js__WEBPACK_IMPORTED_MODULE_2__.removeClassName(delBtn, 'active');\n      }\n    });\n    if (colIndex === 0 && rowIndex === 0 || colIndex === colLen - 1 && rowIndex === rowLen - 1 && !table.className.includes('solid') || (_util_js__WEBPACK_IMPORTED_MODULE_2__.hasClassName(table, 'bubble') || _util_js__WEBPACK_IMPORTED_MODULE_2__.hasClassName(table, 'scatter') || _util_js__WEBPACK_IMPORTED_MODULE_2__.hasClassName(table, 'pie')) && rowIndex === 0) return;\n    if (typeof rowIndex !== 'undefined' && typeof colIndex != 'undefined') {\n      inputBox.setAttribute('style', `\n                left: ${targettd.offsetLeft}px;\n                top: ${targettd.offsetTop + table.offsetTop}px;\n                display: block;\n            `);\n      _util_js__WEBPACK_IMPORTED_MODULE_2__.setAttributes(input, {\n        row: rowIndex,\n        col: colIndex,\n        style: `\n                    width: ${targettd.offsetWidth}px;\n                    height: ${targettd.offsetHeight}px;\n            `\n      });\n    }\n    input.value = targettd.innerText;\n    input.focus();\n    input.select();\n    input.addEventListener('blur', e => {\n      inputBox.style.display = 'none';\n      if (e.target.getAttribute('col') == colLen - 1 && !table.className.includes('solid')) {\n        document.querySelector('#po-chartdata-container table').scrollIntoView({\n          block: 'nearest',\n          inline: 'end',\n          behavior: 'smooth'\n        });\n      } else if (e.target.getAttribute('row') == rowLen - 1) {\n        document.querySelector('#po-chartdata-container table').scrollIntoView({\n          block: 'end',\n          inline: 'nearest',\n          behavior: 'smooth'\n        });\n      }\n    });\n  }\n  changeChartData(params = {}, callback = null) {\n    const {\n      type\n    } = params;\n    const table = document.querySelector('#po-chartdata-container>table');\n    const rows = table.rows;\n    const seriesNames = [];\n    const rowLen = rows.length;\n    let xField;\n    let yField;\n    if (this.ChartType === 'bar') {\n      xField = this.Options.yField;\n      yField = this.Options.xField;\n    } else if (this.ChartType === 'pie') {\n      xField = this.Options.colorField;\n      yField = this.Options.angleField;\n    } else {\n      xField = this.Options.xField;\n      yField = this.Options.yField;\n    }\n    if (type === 'cell') {\n      //修改单元格时更新数据\n      this.changeChartData({\n        type: 'all'\n      }, callback);\n    } else if (type === 'all') {\n      //点击按钮修改所有数据\n      let data = [];\n      let i = 0;\n      Array.from(rows[0].cells).forEach(item => {\n        if (item.innerText !== '') seriesNames.push(item.innerText);\n      });\n      Array.from(rows).forEach((rItem, rIndex) => {\n        const colLen = rItem.cells.length;\n        if (rIndex !== 0 && rIndex !== rowLen - 1) {\n          if (this.ifFixedTable()) data[rIndex - 1] = {};\n          Array.from(rItem.cells).forEach((cItem, cIndex) => {\n            const cellData = _util_js__WEBPACK_IMPORTED_MODULE_2__.tryStr2Num(cItem.innerText);\n            if (this.ifFixedTable()) {\n              if (cIndex === 0) {\n                data[rIndex - 1][xField] = cellData;\n              } else if (cIndex == colLen - 1 && this.isScatterType()) {\n                data[rIndex - 1][seriesNames[cIndex - 1]] = cItem.innerText;\n              } else {\n                data[rIndex - 1][seriesNames[cIndex - 1]] = cellData;\n              }\n            } else {\n              if (cIndex !== 0 && cIndex !== colLen - 1) {\n                const xName = cItem.parentNode.children[0].innerText;\n                data[i] = data[i] || {};\n                data[i][xField] = xName;\n                data[i][yField] = cellData;\n                //一个系列变成多系列时，自动加系列标识’seriesField‘\n                if (!this.isRandomBar()) {\n                  if (!this.Options.seriesField) this.Options.seriesField = 'seriesField';\n                  data[i][this.Options.seriesField] = seriesNames[cIndex - 1];\n                } else {\n                  data[i]['seriesField'] = seriesNames[cIndex - 1];\n                }\n                i++;\n              }\n            }\n          });\n        }\n      });\n      this.Options.data = data;\n      this.draw();\n      if (this.Options.seriesField === 'seriesField') {\n        if (callback) callback({\n          data,\n          seriesField: 'seriesField'\n        });\n      } else {\n        if (callback) callback({\n          data\n        });\n      }\n    }\n  }\n  dataTransToTableData(data = this.Options.data, options = this.Options, type = this.ChartType) {\n    //原始数据转换成表格数据格式\n    const {\n      seriesField,\n      xField,\n      yField,\n      sizeField,\n      colorField,\n      angleField,\n      groupField\n    } = options;\n    let tableData;\n    if (!seriesField || this.ifFixedTable()) {\n      // [一个系列, 固定列] 按照原始数据显示\n      //流程图中的data数据列顺序会打乱\n      tableData = new Array(data.length + 1);\n      if (this.isPieType()) {\n        tableData[0] = [colorField, angleField];\n      } else if (this.isScatterType()) {\n        tableData[0] = [xField, yField, sizeField, colorField];\n      } else if (this.isBarType()) {\n        tableData[0] = [yField, xField];\n      } else {\n        tableData[0] = [xField, yField];\n      }\n      for (let i = 0; i < data.length; i++) {\n        const item = data[i];\n        tableData[i + 1] = tableData[i + 1] || [];\n        if (this.isPieType()) {\n          tableData[i + 1] = [item[colorField], item[angleField]];\n        } else if (this.isScatterType()) {\n          tableData[i + 1] = [item[xField], item[yField], item[sizeField], item[colorField]];\n        } else if (this.isBarType()) {\n          tableData[i + 1] = [item[yField], item[xField]];\n        } else {\n          tableData[i + 1] = [item[xField], item[yField]];\n        }\n      }\n    } else {\n      //多系列\n      let seriesNames = [];\n      let xNames = [];\n      let _seriesField;\n      const _xField = type == 'bar' ? yField : xField;\n      const _yField = type == 'bar' ? xField : yField;\n      if (this.isRandomBar()) {\n        _seriesField = groupField;\n      } else _seriesField = seriesField;\n      data.map(item => {\n        if (!seriesNames.includes(item[_seriesField])) seriesNames.push(item[_seriesField]);\n        if (!xNames.includes(item[_xField])) xNames.push(item[_xField]);\n      });\n      tableData = new Array(xNames.length + 1);\n      tableData[0] = [_xField, ...seriesNames];\n      for (let i = 0; i < xNames.length; i++) {\n        tableData[i + 1] = tableData[i + 1] || [];\n        tableData[i + 1].push(xNames[i]);\n        for (let n of seriesNames) {\n          const v = data.filter(item => item[_xField] == xNames[i] && item[_seriesField] == n);\n          if (v.length > 0) tableData[i + 1].push(v[0][_yField]);else tableData[i + 1].push(0);\n        }\n      }\n    }\n    return tableData;\n  }\n  createConfigModal(args, dom) {\n    const {\n      type,\n      clientX,\n      clientY\n    } = args;\n    if (dom) {\n      dom.style.cssText = \"display: block;top: \" + (clientY + 'px') + \";left: \" + (clientX + 'px') + \";\";\n    }\n    dom.innerHTML = this.configTranstoHtml(this.config, type);\n    this.initChangeEvent(this.config, type);\n  }\n  configTranstoHtml(config = {}, type) {\n    const thisConfig = config[type] || [];\n    let s = '';\n    if (thisConfig.length > 0) {\n      for (let item of thisConfig) {\n        const formType = item.type;\n        const idStr = item.id ? 'id=\"' + item.id + '\"' : '';\n        const labelTxt = item.label ?? '';\n        const option = item.option ?? [];\n        const ifChecked = item.checked ? 'checked' : '';\n        const valueStr = item.value ? 'value=\"' + item.value + '\"' : '';\n        const nameStr = item.name ? 'name=\"' + item.name + '\"' : '';\n        s += '<div>';\n        switch (formType) {\n          case 'select':\n            s += `${labelTxt}<select ${idStr}>`;\n            for (let n of option) {\n              s += `<option value='${n.value}'>${n.txt}</option>`;\n            }\n            s += '</select>';\n            break;\n          case 'devi':\n            s += '<div class=\"devi\"></div>';\n            break;\n          default:\n            s += `${labelTxt}<input type='${formType}' ${nameStr} ${idStr} ${valueStr} ${ifChecked}>`;\n            break;\n        }\n        s += '</div>';\n      }\n    }\n    return s;\n  }\n  initChangeEvent(config = {}, type) {\n    //初始化表单绑定change事件\n    const thisConfig = config[type] || [];\n    for (let item of thisConfig) {\n      if (!item.ifBind) {\n        this.bindChangeEvent({\n          id: item.id,\n          formType: item.type,\n          pro: item.pro\n        });\n      }\n    }\n  }\n  bindChangeEvent(param = {}) {\n    //配置表单绑定change事件，并更新图形\n    const {\n      id,\n      formType,\n      pro\n    } = param;\n    const formDom = document.getElementById(id);\n    const proArr = pro ? pro.split('.') : [];\n    //初始化表单值\n    const defaultVal = this.getDefaultVal(proArr);\n    if (defaultVal) {\n      switch (formType) {\n        case 'checkbox':\n          formDom.checked = defaultVal;\n          break;\n        default:\n          formDom.value = defaultVal;\n          break;\n      }\n    }\n    if (formDom) {\n      formDom.addEventListener('change', event => {\n        let thisVal;\n        switch (formType) {\n          case 'checkbox':\n            thisVal = event.target.checked;\n            break;\n          case 'number':\n            thisVal = Number(event.target.value);\n            break;\n          default:\n            thisVal = event.target.value;\n            break;\n        }\n        this.insertOptions(this.Options, proArr, thisVal);\n        this.draw();\n      });\n    }\n  }\n  insertOptions(obj, params, thisVal) {\n    const p = [...params];\n    const key = p.shift();\n    if (p.length > 0) {\n      if (!obj[key]) obj[key] = {};\n      obj[key] = this.insertOptions(obj[key], p, thisVal);\n    } else {\n      obj[key] = thisVal;\n    }\n    return obj;\n  }\n  getDefaultVal(params, obj = this.Options) {\n    const p = [...params];\n    const key = p.shift();\n    if (obj[key]) {\n      if (Object.prototype.toString.call(obj[key]) === '[object Object]') {\n        return this.getDefaultVal(p, obj[key]);\n      } else {\n        return obj[key];\n      }\n    } else return;\n  }\n  bindMouseMoveEvent(dom) {\n    let x = 0;\n    let y = 0;\n    let l = 0;\n    let t = 0;\n    let isMoving = false;\n    dom.onmousedown = function (e) {\n      if (e.button === 0) {\n        x = e.clientX;\n        y = e.clientY;\n        l = dom.offsetLeft;\n        t = dom.offsetTop;\n        isMoving = true;\n        dom.style.cursor = 'move';\n        window.onmousemove = function (e) {\n          if (isMoving == false) return false;\n          var nx = e.clientX;\n          var ny = e.clientY;\n          var nl = nx - (x - l);\n          var nt = ny - (y - t);\n          dom.style.position = 'absolute';\n          dom.style.left = nl + 'px';\n          dom.style.top = nt + 'px';\n        };\n      }\n    };\n    dom.onmouseup = function () {\n      isMoving = false;\n      dom.style.cursor = 'default';\n      window.onmousemove = null;\n    };\n  }\n  isLineType() {\n    return ['line'].includes(this.ChartType);\n  }\n  isScatterType() {\n    return ['scatter', 'bubble'].includes(this.ChartType);\n  }\n  isBarType() {\n    return ['bar'].includes(this.ChartType);\n  }\n  isPieType() {\n    return ['pie'].includes(this.ChartType);\n  }\n  ifFixedTable() {\n    //[气泡图, 散点图, 饼图] 数据列固定\n    return this.isScatterType() || this.isPieType();\n  }\n  isRandomBar() {\n    return this.ChartType === 'bar' && this.Options?.seriesField === this.Options?.yField;\n  }\n  delIconNotAllowed() {\n    const delBtn = document.getElementById('del-icon');\n    _util_js__WEBPACK_IMPORTED_MODULE_2__.removeClassName(delBtn, 'active');\n    delBtn.querySelectorAll('svg>path').forEach(item => {\n      item.setAttribute('fill', '#C7C9CB');\n    });\n  }\n  delIconAllowed() {\n    const delBtn = document.getElementById('del-icon');\n    _util_js__WEBPACK_IMPORTED_MODULE_2__.tryAddClassName(delBtn, 'active');\n    delBtn.querySelectorAll('svg>path').forEach(item => {\n      item.setAttribute('fill', '#212930');\n    });\n  }\n  //清空th时默认当前系列N最大的系列N+1\n  clearThNames(obj) {\n    const table = document.querySelector('#po-chartdata-container>table');\n    let thNames = [];\n    table.querySelectorAll('tr:first-child>td').forEach((item, i) => {\n      if (item.innerText !== '' && i !== obj.cellIndex) {\n        thNames.push(item.innerText);\n      }\n    });\n    let maxNum = 0;\n    thNames.forEach(item => {\n      if (/^系列\\d+$/.test(item)) {\n        const numStr = item.replace('系列', '');\n        const num = parseInt(numStr);\n        if (num > maxNum) maxNum = num;\n      }\n    });\n    obj.innerText = '系列' + (maxNum + 1);\n  }\n  //清空th时默认当前类别N最大的类别N+1\n  clearTrNames(obj) {\n    const table = document.querySelector('#po-chartdata-container>table');\n    let trNames = [];\n    table.querySelectorAll('tr>td:first-child').forEach((item, i) => {\n      if (item.innerText !== '' && i !== obj.cellIndex) {\n        trNames.push(item.innerText);\n      }\n    });\n    let maxNum = 0;\n    trNames.forEach(item => {\n      if (/^类别\\d+$/.test(item)) {\n        const numStr = item.replace('类别', '');\n        const num = parseInt(numStr);\n        if (num > maxNum) maxNum = num;\n      }\n    });\n    obj.innerText = '类别' + (maxNum + 1);\n  }\n  ifTrRepeatLabel(value) {\n    const table = document.querySelector('#po-chartdata-container>table');\n    let trNames = [];\n    table.querySelectorAll('tr>td:first-child').forEach((item, i) => {\n      trNames.push(item.innerText);\n    });\n    return trNames.includes(value);\n  }\n  ifThRepeatLabel(value) {\n    const table = document.querySelector('#po-chartdata-container>table');\n    let thNames = [];\n    table.querySelectorAll('tr:first-child>td').forEach((item, i) => {\n      thNames.push(item.innerText);\n    });\n    return thNames.includes(value);\n  }\n}\n\n//# sourceURL=webpack://my-webpack-project/./app/websrc/view/js/base/base.js?")},"./app/websrc/view/js/data.js":function appWebsrcViewJsDataJs(__unused_webpack_module,__webpack_exports__,__webpack_require__){"use strict";eval("__webpack_require__.r(__webpack_exports__);\n/* harmony export */ __webpack_require__.d(__webpack_exports__, {\n/* harmony export */   \"basicConfig\": () => (/* binding */ basicConfig),\n/* harmony export */   \"columnConfig\": () => (/* binding */ columnConfig),\n/* harmony export */   \"defaultBarData\": () => (/* binding */ defaultBarData),\n/* harmony export */   \"defaultColors\": () => (/* binding */ defaultColors),\n/* harmony export */   \"defaultColumnData\": () => (/* binding */ defaultColumnData),\n/* harmony export */   \"defaultData\": () => (/* binding */ defaultData),\n/* harmony export */   \"defaultPieData\": () => (/* binding */ defaultPieData),\n/* harmony export */   \"defaultRadarData\": () => (/* binding */ defaultRadarData),\n/* 