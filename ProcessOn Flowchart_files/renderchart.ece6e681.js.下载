\n      var v2 = gl_matrix__WEBPACK_IMPORTED_MODULE_0__.scale([0, 0], v, d1);\n      cp1 = gl_matrix__WEBPACK_IMPORTED_MODULE_0__.add([0, 0], point, v1);\n      nextCp0 = gl_matrix__WEBPACK_IMPORTED_MODULE_0__.add([0, 0], point, v2);\n      // 下一个控制点必须在这个点和下一个点之间\n      nextCp0 = gl_matrix__WEBPACK_IMPORTED_MODULE_0__.min([0, 0], nextCp0, gl_matrix__WEBPACK_IMPORTED_MODULE_0__.max([0, 0], nextPoint, point));\n      nextCp0 = gl_matrix__WEBPACK_IMPORTED_MODULE_0__.max([0, 0], nextCp0, gl_matrix__WEBPACK_IMPORTED_MODULE_0__.min([0, 0], nextPoint, point));\n      // 重新计算 cp1 的值\n      v1 = gl_matrix__WEBPACK_IMPORTED_MODULE_0__.sub([0, 0], nextCp0, point);\n      v1 = gl_matrix__WEBPACK_IMPORTED_MODULE_0__.scale([0, 0], v1, -d0 / d1);\n      cp1 = gl_matrix__WEBPACK_IMPORTED_MODULE_0__.add([0, 0], point, v1);\n      // 上一个控制点必须要在上一个点和这一个点之间\n      cp1 = gl_matrix__WEBPACK_IMPORTED_MODULE_0__.min([0, 0], cp1, gl_matrix__WEBPACK_IMPORTED_MODULE_0__.max([0, 0], prevPoint, point));\n      cp1 = gl_matrix__WEBPACK_IMPORTED_MODULE_0__.max([0, 0], cp1, gl_matrix__WEBPACK_IMPORTED_MODULE_0__.min([0, 0], prevPoint, point));\n      // 重新计算 nextCp0 的值\n      v2 = gl_matrix__WEBPACK_IMPORTED_MODULE_0__.sub([0, 0], point, cp1);\n      v2 = gl_matrix__WEBPACK_IMPORTED_MODULE_0__.scale([0, 0], v2, d1 / d0);\n      nextCp0 = gl_matrix__WEBPACK_IMPORTED_MODULE_0__.add([0, 0], point, v2);\n      if (hasConstraint) {\n        cp1 = gl_matrix__WEBPACK_IMPORTED_MODULE_0__.max([0, 0], cp1, min);\n        cp1 = gl_matrix__WEBPACK_IMPORTED_MODULE_0__.min([0, 0], cp1, max);\n        nextCp0 = gl_matrix__WEBPACK_IMPORTED_MODULE_0__.max([0, 0], nextCp0, min);\n        nextCp0 = gl_matrix__WEBPACK_IMPORTED_MODULE_0__.min([0, 0], nextCp0, max);\n      }\n      cps.push(cp0);\n      cps.push(cp1);\n      cp0 = nextCp0;\n    }\n  }\n  if (isLoop) {\n    cps.push(cps.shift());\n  }\n  return cps;\n}\n/**\n * create bezier spline from catmull rom spline\n * @param {Array} crp Catmull Rom Points\n * @p